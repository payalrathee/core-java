package arrayLists;
import java.util.*;
import utilities.*;

public class ArrayList1 {

	public static void main(String[] args) {
		ArrayList<String> a=new ArrayList<>();
		a.add("a");
		a.add("b");
		a.add("c");
		a.add("d");
		a.add(1,"z");
		
		a.set(1, "payal");
		
		System.out.println(a);
		
		for(String s:a)
		{
			if(s.equals("a"))
			{
				a.remove(s);
				break;
			}
			
		}
		System.out.println(a);
		
		ArrayList<String> b=new ArrayList<>();
		
		
	}
}
